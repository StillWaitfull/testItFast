<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Google_suite" parallel="tests">
    <test name="TestsChrome" parallel="methods" thread-count="2">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="tests.GoogleTest"/>
        </classes>
    </test>
    <test name="TestsFirefox" parallel="methods" thread-count="2">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="tests.GoogleTest"/>
        </classes>
    </test>
</suite>